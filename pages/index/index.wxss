.swiper_index navigator image {
  width: 100%;
}
.cat_index {
  display: flex;
}
.cat_index navigator {
  flex: 1;
  padding: 20rpx;
}
.cat_index navigator image {
  width: 100%;
  height: 100%;
}
.floor_group {
  overflow: hidden;
}
.floor_group .floor_title {
  padding: 10rpx 0;
}
.floor_group .floor_title image {
  width: 100%;
}
.floor_group .floor_list navigator {
  float: left;
  width: 33vw;
}
.floor_group .floor_list navigator:nth-last-child(-n + 4) {
  height: 27.45258621vw;
  border-left: 3rpx solid #fff;
}
.floor_group .floor_list navigator:nth-child(2),
.floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 3rpx solid #fff;
}
.floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
